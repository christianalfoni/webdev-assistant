{
  "name": "MV3 Migration - content script example",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["http://*:*/*"],
      "js": ["content-script.js"],
      "world": "MAIN",
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
